generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

model Article {
  id            Int @id @default(autoincrement())
  title         String?
  content       String?
  url           String
  dataSourceId  String?
  timestamp     String
}
